    "from sklearn.preprocessing import MinMaxScaler\n",
    "import seaborn as sns\n",
    "\n",
    "crashes = sns.load_dataset('car_crashes')\n",
    "scaler = MinMaxScaler()\n",
    "crash_standard = scaler.fit_transform(crashes.iloc[:, 0:-1]) # min-max\n",
