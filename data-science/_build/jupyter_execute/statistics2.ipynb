    "\n",
    "# \n",
    "#  excel  pandas \n",
    "df = pd.read_excel(\"datas/data-pca.xlsx\", index_col=0)\n",
    "\n",
    "scale_values = MinMaxScaler().fit_transform(df.values)  # \n",
    "\n",
    "kmeans = KMeans(n_clusters=3).fit(scale_values)  #  3 ,  n_init= 'auto' \n",
    "print(kmeans.labels_)  # \n",
    "\n",
    "# \n",
    "cluster_1 = []\n",
    "cluster_2 = []\n",
    "cluster_3 = []\n",
    "for i, j in enumerate(kmeans.labels_):\n",
    "    if j == 0:\n",
    "        cluster_1.append(df.index[i])\n",
    "    elif j == 1:\n",
    "        cluster_2.append(df.index[i])\n",
    "    else:\n",
    "        cluster_3.append(df.index[i])\n",
    "print(\"1\")\n",
    "print(cluster_1)\n",
    "print(\"2\")\n",
    "print(cluster_2)\n",
    "print(\"3\")\n",
    "print(cluster_3)\n",
